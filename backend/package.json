{
  "name": "prequaliq-backend",
  "version": "1.0.0",
  "description": "PrequaliQ Backend API",
  "main": "server.js",
  "scripts": {
    "start": "node scripts/runMigrations.js && node scripts/ensureAdmin.js && (node scripts/seedCpvFromApi.js || true) && node server.js",
    "dev": "nodemon server.js",
    "migrate": "sequelize-cli db:migrate",
    "force-migrate": "node scripts/forceMigrations.js",
    "seed": "sequelize-cli db:seed:all",
    "create-admin": "node scripts/createAdmin.js",
    "clean-db": "node scripts/cleanDatabase.js",
    "seed-prod-questionnaires": "node scripts/seedProdQuestionnaires.js",
    "seed-cpv-from-api": "node scripts/seedCpvFromApi.js",
    "seed-cpv-prod": "NODE_ENV=production node scripts/seedCpvFromApi.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^8.0.1",
    "resend": "^3.2.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2"
  }
}
